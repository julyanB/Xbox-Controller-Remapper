{
  "name": "controllerrebinder-desktop",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron electron/main.js",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=production electron electron/main.js"
  },
  "dependencies": {
    "electron-store": "^10.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^32.1.2",
    "typescript": "^5.4.5",
    "vite": "^4.5.3",
    "wait-on": "^7.2.0"
  }
}
